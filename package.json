{
  "name": "electron-webpack-react-sass",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron ./dist",
    "dev": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "package:mac": "electron-packager ./dist electron-webpack-react-sass --platform=darwin --arch=x64 --out=./build/mac --overwrite --asar",
    "package:win": "electron-packager ./dist electron-webpack-react-sass --platform=win32 --arch=x64 --out=./build/win --overwrite --asar",
    "package:linux": "electron-packager ./dist electron-webpack-react-sass --platform=linux --arch=x64 --out=./build/linux --overwrite --asar"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^2.0.3",
    "electron-packager": "^12.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  },
  "engines": {
    "node": ">= 8.0.0"
  }
}
